{
  "name": "alexa-dev-dog-fact-skills",
  "version": "1.0.1",
  "description": "an alexa skill to give motivational dog themed facts for developers",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf build",
    "build": "npm-run-all --sequential build:*",
    "build:npmcheck": "path-exists node_modules || exit 1",
    "build:mkdir": "npm run clean && mkdir build",
    "build:zip": "bestzip build/bundle.zip ./index.js ./facts.js ./node_modules",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier-eslint \"index.js\" \"facts.js\" --write",
    "lint": "eslint -f './node_modules/eslint-formatter-pretty' index.js facts.js"
  },
  "dependencies": {
    "alexa-sdk": "1.0.25",
    "axios": "^0.19.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "bestzip": "^2.1.5",
    "eslint": "^6.6.0",
    "eslint-config-xo-space": "^0.21.0",
    "eslint-formatter-pretty": "^2.1.1",
    "mkdirp": "0.5.1",
    "npm-run-all": "^4.1.5",
    "path-exists-cli": "1.0.0",
    "prettier-eslint-cli": "^4.3.0",
    "rimraf": "^3.0.0"
  },
  "author": "Eric McCormick (https://ericmccormick.io/)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edm00se/developer-dog-alexa-skill.git"
  },
  "license": "MIT"
}
